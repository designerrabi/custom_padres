<div class="tabs_wrapper_area">
  <div class="container">
    <div class="row">
      <div class="col-lg-12 text-center">
        <div class="global-section-title">
          <h2>{{ section.settings.section_title }}</h2>
        </div>
      </div>
    </div>
{% assign weeks = "Saturday,Sunday,Monday,Tuesday,Wednesday,Thursday,Friday" | split: "," %}
    <div class="row">
      <div class="col-lg-12">
        <div class="tabs_nav_wrapper">
          <ul class="nav nav-pills mb-3" id="pills-tab--{{ section.id }}" role="tablist">
            {% for week in weeks %}
            <li class="nav-item" role="presentation">
              <button
                class="nav-link {% if forloop.first %}active{% endif %}"
                id="pills-{{ week | handle }}-tab-{{ section.id }}"
                data-bs-toggle="pill"
                data-bs-target="#pills-{{ week | handle }}-{{ section.id }}"
                type="button"
                role="tab"
                aria-controls="pills-{{ week | handle }}-{{ section.id }}"
                aria-selected="{% if forloop.first %}true {% else %}false{% endif %}"
              >
                {{ week }}
              </button>
            </li>
            {% endfor %}
          </ul>
        </div>
        <div class="tabs_content_wrapper">
          <div class="tab-content" id="pills-tabContent--{{ section.id }}">
            {% for week in weeks %}
            <div
              class="tab-pane fade {% if forloop.first %}show active{% endif %}"
              id="pills-{{ week | handle }}-{{ section.id }}"
              role="tabpanel"
              aria-labelledby="pills-{{ week | handle }}-tab-{{ section.id }}"
              tabindex="0"
            >
              <div class="tabs_content_inner">
                <div class="tabs_content_header">
                  <h3>{{ week }}</h3>
                  <div class="tabs_content_header-list d-none">
                    <ul>
                      <li>Frontier Park</li>
                      <li>Lewis Boat House</li>
                      <li>Historic Main Street</li>
                      <li>Lewis and Clark Monument</li>
                      <li>
                        <a href="https://www.discoverstcharles.com/things-to-do/attractions/st-charles-trolley/"
                          >St. Charles Trolley</a
                        >
                      </li>
                    </ul>
                  </div>
                </div>
                {% for block in section.blocks %}
                  {% if block.type == 'event_tab' and block.settings.day == week %}
                    <div class="row event_bg">
                      <div class="col-lg-6 col-md-6">
                        <div class="tabs_book_event_left_content">
                          <div class="row">
                            <div class="col-lg-5">
                              <div class="tabs_book_event_left_inner">
                                {{ block.settings.event_address }}
                              </div>
                              <div class="tabs_book_event_left_inner_middle">
                                <h4>Lodging:</h4>
                                <span>{{ block.settings.loading }}</span>
                              </div>
                            </div>
                            <div class="col-lg-7">
                              <div class="tabs_book_event_right">
                                <h3>Meals:</h3>
                                <p>{{ block.settings.meals }}</p>
                              </div>
                              <div class="tabs_book_event_left_inner_middle">
                                <h4>Points of Interest:</h4>
                                {{ block.settings.points_of_interest }}
                              </div>
                            </div>
                          </div>
                          <div class="tabs_book_event_left_inner_content">
                            {{ block.settings.event_description }}
                          </div>
                          <div class="primary_btn_wrp">
                            <a
                              class="primary_btn"
                              href="{{ block.settings.register_btn_url }}"
                              >Register Now
                              <svg
                                stroke="currentColor"
                                fill="currentColor"
                                stroke-width="0"
                                viewBox="0 0 448 512"
                                height="1em"
                                width="1em"
                                xmlns="http://www.w3.org/2000/svg"
                              >
                                <path d="M438.6 278.6c12.5-12.5 12.5-32.8 0-45.3l-160-160c-12.5-12.5-32.8-12.5-45.3 0s-12.5 32.8 0 45.3L338.8 224 32 224c-17.7 0-32 14.3-32 32s14.3 32 32 32l306.7 0L233.4 393.4c-12.5 12.5-12.5 32.8 0 45.3s32.8 12.5 45.3 0l160-160z"></path>
                              </svg>
                            </a>
                          </div>
                        </div>
                      </div>
                      <div class="col-lg-6 col-md-6">
                        <div class="book_event_right_slider">
                          <div class="tabs--{{ week | handle }}">
                            {% if block.settings.event_slider_image1 != blank %}
                            <div>
                              <div class="event_right_slider_item">
                             <img src="{{ block.settings.event_slider_image1 | image_url }}" alt="event slider image">

                              </div>
                            </div>
                            {% endif %}
                            {% if block.settings.event_slider_image2 != blank %}
                            <div>
                              <div class="event_right_slider_item">
                                <img src="{{ block.settings.event_slider_image2 | image_url }}" alt="event slider image">
                              </div>
                            </div>
                            {% endif %}
                            {% if block.settings.event_slider_image3 != blank %}
                            <div>
                              <div class="event_right_slider_item">
                                <img src="{{ block.settings.event_slider_image3 | image_url }}" alt="event slider image">
                              </div>
                            </div>
                            {% endif %}
                            {% if block.settings.event_slider_image4 != blank %}
                            <div>
                              <div class="event_right_slider_item">
                                <img src="{{ block.settings.event_slider_image4 | image_url }}" alt="event slider image">
                              </div>
                            </div>
                            {% endif %}
                          </div>
                        </div>
                      </div>
                    </div>
                    <div class="vacation_title">
                      <h2>{{ block.settings.vacation_title }}</h2>
                    </div>
                    <div class="row vacation_bg">
                      <div class="col-lg-6 col-md-6">
                        <div class="vacation_price_box">
                          <div class="vacation_price_box_header">
                            <h3>{{ block.settings.vacation_box1_heading }}</h3>
                          </div>
                          <div class="vacation_price_box_list">
                            {{ block.settings.vacation_box1_description }}
                          </div>
                        </div>
                      </div>
                      <div class="col-lg-6 col-md-6">
                        <div class="vacation_price_box">
                          <div class="vacation_price_box_header">
                            <h3>{{ block.settings.vacation_box2_heading }}</h3>
                          </div>
                          <div class="vacation_price_box_list">
                            {{ block.settings.vacation_box2_description }}
                          </div>
                        </div>
                      </div>
                    </div>

                    <div class="tabs_gallery_slider">
                      <div class="tabs_gallery--{{ week | handle }}">
                        {% if block.settings.gallery_slider_image1 != blank %}
                        <div>
                          <div class="gallery_thumb_item_seven">
                            <img src="{{ block.settings.gallery_slider_image1 | image_url }}" alt="gallery slider">
                          </div>
                        </div>
                        {% endif %}
                        {% if block.settings.gallery_slider_image2 != blank %}
                        <div>
                          <div class="gallery_thumb_item_seven">
                            <img src="{{ block.settings.gallery_slider_image2 | image_url }}" alt="gallery slider">
                          </div>
                        </div>
                        {% endif %}
                        {% if block.settings.gallery_slider_image3 != blank %}
                        <div>
                          <div class="gallery_thumb_item_seven">
                            <img src="{{ block.settings.gallery_slider_image3 | image_url }}" alt="gallery slider">
                          </div>
                        </div>
                        {% endif %}
                        {% if block.settings.gallery_slider_image4 != blank %}
                        <div>
                          <div class="gallery_thumb_item_seven">
                            <img src="{{ block.settings.gallery_slider_image4 | image_url }}" alt="gallery slider">
                          </div>
                        </div>
                        {% endif %}
                      </div>
                      <button class="tabs_gallery_nav_prev active-arrow-prev--{{ week | handle }}">
                        <svg
                          stroke="currentColor"
                          fill="currentColor"
                          stroke-width="0"
                          viewBox="0 0 320 512"
                          height="1em"
                          width="1em"
                          xmlns="http://www.w3.org/2000/svg"
                        >
                          <path d="M34.52 239.03L228.87 44.69c9.37-9.37 24.57-9.37 33.94 0l22.67 22.67c9.36 9.36 9.37 24.52.04 33.9L131.49 256l154.02 154.75c9.34 9.38 9.32 24.54-.04 33.9l-22.67 22.67c-9.37 9.37-24.57 9.37-33.94 0L34.52 272.97c-9.37-9.37-9.37-24.57 0-33.94z"></path>
                        </svg>
                      </button>
                      <button class="tabs_gallery_nav_next active-arrow-next--{{ week | handle }}">
                        <svg
                          stroke="currentColor"
                          fill="currentColor"
                          stroke-width="0"
                          viewBox="0 0 320 512"
                          height="1em"
                          width="1em"
                          xmlns="http://www.w3.org/2000/svg"
                        >
                          <path d="M285.476 272.971L91.132 467.314c-9.373 9.373-24.569 9.373-33.941 0l-22.667-22.667c-9.357-9.357-9.375-24.522-.04-33.901L188.505 256 34.484 101.255c-9.335-9.379-9.317-24.544.04-33.901l22.667-22.667c9.373-9.373 24.569-9.373 33.941 0L285.475 239.03c9.373 9.372 9.373 24.568.001 33.941z"></path>
                        </svg>
                      </button>
                      <!-- <div class="tabs-global-one-pagination"></div> -->
                    </div>
                  {% endif %}
                {% endfor %}
                
              </div>
            </div>
            {% endfor %}
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

<script src="https://code.jquery.com/jquery-3.7.1.min.js"></script>
<script type="text/javascript" src="https://cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.min.js"></script>

<script>
   $(document).ready(function(){
      {% for week in weeks %}
       $('.tabs--{{ week | handle }}').slick({
           slidesToShow: 1,
           slidesToScroll: 1,
           dots: true,
           arrows: false,
           autoplay: true,
           centerMode: true,
           centerPadding: '0px',
           autoplaySpeed: 3000,
           infinite: true,

           responsive: [
               {
                   breakpoint: 1200,
                   settings: {
                       slidesToShow: 1
                   }
               },
               {
                   breakpoint: 991,
                   settings: {
                       slidesToShow: 1
                   }
               },
               {
                   breakpoint: 768,
                   settings: {
                       slidesToShow: 1
                   }
               },
               {
                   breakpoint: 767,
                   settings: {
                       slidesToShow: 1,
                   }
               }
           ]
       });
      

       // Tabs Gallery Slider JS

       $('.tabs_gallery--{{ week | handle }}').slick({
           slidesToShow: 3,
           slidesToScroll: 1,
           dots: true,
           arrows: true,
           autoplay: true,
           centerMode: false,
           centerPadding: '10px',
           autoplaySpeed: 3000,
           infinite: true,
           prevArrow: $('.active-arrow-prev--{{ week | handle }}'),
           nextArrow: $('.active-arrow-next--{{ week | handle }}'),
           responsive: [
               {
                   breakpoint: 1200,
                   settings: {
                       slidesToShow: 3
                   }
               },
               {
                   breakpoint: 991,
                   settings: {
                       slidesToShow: 2
                   }
               },
               {
                   breakpoint: 768,
                   settings: {
                       slidesToShow: 1
                   }
               },
               {
                   breakpoint: 767,
                   settings: {
                       slidesToShow: 1,
                   }
               }
           ]
       });

      {% endfor %}

  });
</script>

