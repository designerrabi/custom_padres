       .tabs_nav_wrapper {
           margin-top: 50px;
           margin-bottom: 50px;
       }
       .tabs_nav_wrapper ul {
           justify-content: center;
           gap: 15px;
       }
       .tabs_nav_wrapper ul li button.nav-link {
           border-radius: 10px !important;
           border: 2px solid #5A6376 !important;
           background: #FFF;
           padding: 16px 20px;
           font-size: 22px;
           font-weight: 500;
           line-height: 20px;
           color: #494C54;
           position: relative;
           z-index: 1;
           font-family: "Inter", sans-serif !important;
       }
       .tabs_nav_wrapper ul li button.nav-link.active {
           border-radius: 10px !important;
           color: #FFF;
           background: #FFF;
           position: relative;
           border: 2px solid transparent !important;
           background-clip: padding-box;
       }

       .tabs_nav_wrapper ul li button.nav-link.active::before {
           content: '';
           position: absolute;
           top: -2px;
           left: -2px;
           right: -2px;
           bottom: -2px;
           background: linear-gradient(90deg, #5FD56A 0%, #02953D 100%);
           border-radius: 10px;
           z-index: -1;
       }
       .tabs_nav_wrapper ul li button.nav-link.active::after{
           content: '';
           position: absolute;
           bottom: -10px;
           left: 0px;
           right: 0px;
           margin: 0 auto;
           clip-path: polygon(100% 0, 0 0, 50% 63%);
           width: 22px;
           height: 9px;
           background: #2FB453;
       }
       .tabs_content_inner {
           border-radius: 20px;
